{
  "name": "pet-marketplace-server",
  "version": "1.0.0",
  "description": "Backend for Pet Marketplace",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon --watch src --ignore src/data --ignore logs --exec node src/server.js",
    "test": "jest --detectOpenHandles",
    "test:analytics": "jest tests/analytics.test.js",
    "test:chatbot": "jest tests/chatbot.test.js",
    "test:payments": "jest tests/payments.test.js",
    "test:scenarios": "jest tests/scenarios.test.js",
    "test:all": "jest tests/",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "analytics:repair": "node src/scripts/repairAnalyticsJSON.js",
    "analytics:backup": "node src/scripts/backupAnalytics.js",
    "analytics:clean": "node src/scripts/cleanOldBackups.js"
  },
  "keywords": [
    "pets",
    "marketplace",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@huggingface/inference": "^4.13.3",
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "connect-pg-simple": "^10.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "express-session": "^1.18.2",
    "express-validator": "^7.3.0",
    "helmet": "^8.1.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.19.4",
    "openai": "^6.9.0",
    "pg": "^8.16.3",
    "stripe": "^19.3.1",
